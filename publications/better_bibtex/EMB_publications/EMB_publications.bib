@article{brunaEffectMutualistPartner2014,
  type = {Article},
  ids = {WOS:000346851400001},
  title = {Effect of Mutualist Partner Identity on Plant Demography},
  author = {Bruna, Emilio M. and Izzo, Thiago J. and Inouye, Brian D. and Vasconcelos, Heraldo L.},
  year = {2014},
  month = dec,
  journal = {Ecology},
  volume = {95},
