@article{lucasSeedlingCotoleranceMultiple2013,
  type = {Article},
  ids = {WOS:000327305900003},
  title = {Seedling Co-Tolerance of Multiple Stressors in a Disturbed Tropical Floodplain Forest},
  author = {Lucas, Christine M. and Bruna, Emilio M. and Nascimento, Cristiane M. N.},
  year = {2013},
  month = jan,
  journal = {Ecosphere},
