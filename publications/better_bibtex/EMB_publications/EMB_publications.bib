  author = {Dattilo, Wesley and Izzo, Thiago J. and Inouye, Brian D. and Vasconcelos, Heraldo L. and Bruna, Emilio M.},
  year = {2009},
  month = sep,
  journal = {Biotropica},
  volume = {41},
  number = {5},
  pages = {642--646},
  publisher = {WILEY},
