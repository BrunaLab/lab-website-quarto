@article{costaAdaptiveForagingLeafcutter2019,
  type = {Article},
  ids = {WOS:000460077500009},
  title = {Adaptive Foraging of Leaf-Cutter Ants to Spatiotemporal Changes in Resource Availability in {{Neotropical}} Savannas},
  author = {Costa, Alan N. and Vasconcelos, Heraldo L. and {Vieira-Neto}, Ernane H.M. and Bruna, Emilio M.},
  year = {2019},
  journal = {Ecological Entomology},
