  keywords-plus = {SEED-DISPERSAL; TEMPORAL VARIATION; INSECT ABUNDANCE; RAIN-FOREST; FRUIT; BIRDS; REMOVAL; FLUCTUATIONS; POLLINATORS; VARIABILITY},
