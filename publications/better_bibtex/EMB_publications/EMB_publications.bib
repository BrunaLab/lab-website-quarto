  author = {Darosci, Adriano Antonio Brito and Bruna, Emilio M. and {Motta-Junior}, José Carlos and Ferreira, Cristiane da Silva and Blake, John Gilman and Munhoz, Cássia Beatriz Rodrigues},
  year = {2017},
  month = oct,
  journal = {Acta Oecologica},
  volume = {84},
