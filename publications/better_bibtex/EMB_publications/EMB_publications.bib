@article{vasconcelosRoadsAlterColonization2006,
  type = {Article},
  ids = {WOS:000239634900010},
  title = {Roads Alter the Colonization Dynamics of a Keystone Herbivore in {{Neotropical}} Savannas},
  author = {Vasconcelos, Heraldo L. and {Vieira-Neto}, Ernane H. M. and Mundim, Fabiane M. and Bruna, Emilio M.},
  year = {2006},
