@article{jurinitzAbioticBioticInfluences2013,
  type = {Article},
  ids = {WOS:000326333400008},
  title = {Abiotic and {{Biotic Influences}} on {{Early-Stage Survival}} in {{Two Shade-Tolerant Tree Species}} in {{Brazil}}'s {{Atlantic Forest}}},
  author = {Jurinitz, Cristiane F. and {de Oliveira}, Alexandre A. and Bruna, Emilio M.},
  year = {2013},
  month = nov,
  journal = {Biotropica},
